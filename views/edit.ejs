<h1>Edit Page</h1>
<form action="/stores/edit/:sid" method="POST">
    <% store.forEach((stores) => { %>
        <label>SID</label>
        <input name="sid" value="<%= stores.sid %>" readonly></input>
        <br>
        <label>Location</label>
        <input name="location" value="<%= stores.location %>" minlength="1"></input>
        <br>
        <label>Manager ID</label>
        <input name="mgrid" value="<%= stores.mgrid %>" maxlength="4"></input>
        <br>
    <% }) %>
    <input type="submit" value="submit"></input>
</form>
